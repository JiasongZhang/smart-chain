
.content{
	width: 100%; 
	overflow: hidden;
	padding: 0;
	background-color: #fff;
}
.state{
	width: 92%;
	margin: 0 auto;
	height: 93rpx;
	line-height: 93rpx;
	font-size: 35rpx !important;
	letter-spacing: 7rpx;
	font-weight: 600;
	color: #545454;
}
._hr{
	color: #545454;
}
.jiaoyi{
	width: 92%;
	margin: 0 auto;
	letter-spacing: 5rpx;
	font-size: 30rpx !important;
	color: #AE3535;
	padding-top: 30rpx;
	padding-bottom: 30rpx;
	line-height: 50rpx;
}
.span{
	width: 120rpx;
	border-bottom:solid 2rpx #AE3535;
	display: block;
	text-align: center;
	height: 40rpx;
}
.jine{
	text-indent: 2.3em;
}
.shoukuan{
	width: 92%;
	margin: 0 auto;
}
.shoukuan .one{
	font-size: 35rpx !important;
	letter-spacing: 7rpx;
	font-weight: 600;
	color: #545454;
}
.shoukuan ._p{
	letter-spacing: 5rpx;
	font-size: 30rpx !important;
	color: #AE3535;
	line-height: 50rpx;
}
.two{
	color: #AE3535;
	font-size: 10rpx;
}
._button{
	width: 450rpx;
	height: 80rpx;
	margin: 0 auto;
	color: #fff;
	border-radius: 10rpx;
	background-color: #2465E7;
	line-height: 80rpx;
	letter-spacing: 8rpx;
	margin-top: 20rpx;
}

